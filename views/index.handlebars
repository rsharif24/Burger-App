<div class="container">
<div id="header">
  <img src="https://cdn.dribbble.com/users/979364/screenshots/2814751/burger_dribbble.png" width="40%">
  <h1>Eat-Da-Burger!</h1>
</div>
<div id="wrapper">
  <div class="col-md-6">
    <ul>
      {{#each burger}} {{#unless this.devoured}}
      <li>
        {{this.burger_name}}

        <form action='/{{this.id}}?_method=PUT' method='POST'>
          <input type="hidden" name="devoured" value="true">
          <button type="submit" class="btn btn-default" id="devour">Devour It</button>
        </form>
      </li>
      {{/unless}} {{/each}}
    </ul>
  </div>


  <div class="col-md-6">
    <ul>
      {{#each burger}} {{#if this.devoured}}
      <li>
        {{this.burger_name}}
      </li>
      {{/if}} {{/each}}
    </ul>
  </div>
</div>
<br><br>

  <form class="create-update-form" action="/" method="POST" id="wrapper2">

    <div class="form-group" id="textboxWrapper">
      <input type="text" id="burgerBox" name="burger_name" class="form-control">
    </div>
<br>
    <button type="submit" class="btn btn-default" id="submit">Submit</button>
    <br><br>
    <p>Enter a Burger You Would Like to Add, Then Click Submit.</p>
  </form>
  <br><br>
</div>